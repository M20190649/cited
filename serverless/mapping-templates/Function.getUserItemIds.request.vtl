{
  "version": "2017-02-28",
  "operation": "Query",
  "query": {
    "expression": "userId = :userId AND begins_with(itemId, :itemIdPrefix)",
    "expressionValues": {
      ":userId": $util.dynamodb.toDynamoDBJson($ctx.prev.result.userId),
      ":itemIdPrefix": $util.dynamodb.toDynamoDBJson($ctx.prev.result.itemIdPrefix)
    }
  }
}