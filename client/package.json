{
  "name": "disco-client",
  "version": "1.0.0",
  "description": "A discovery engine to descover academic papers through references and citations.",
  "author": "Chris Ye <chrisyeshi@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js --host 0.0.0.0",
    "start": "npm run dev",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit test/e2e/specs",
    "build": "node build/build.js",
    "deploy": "node build/deploy.js"
  },
  "dependencies": {
    "aws-amplify": "^1.1.28",
    "aws-amplify-vue": "^0.2.11",
    "aws-appsync": "^1.8.0",
    "axios": "^0.18.0",
    "basicscroll": "^3.0.2",
    "d3-scale": "^2.2.2",
    "d3-scale-chromatic": "^1.3.3",
    "d3-selection": "^1.4.0",
    "d3-shape": "^1.3.4",
    "dexie": "^2.0.4",
    "dotenv-webpack": "^1.7.0",
    "flipping": "^1.1.0",
    "graphql-tag": "^2.10.1",
    "lodash": "^4.17.10",
    "lorem-ipsum": "^1.0.6",
    "material-design-icons-iconfont": "^3.0.3",
    "mysql": "^2.16.0",
    "popper.js": "^1.14.7",
    "repeat-string": "^1.6.1",
    "to-regex-range": "^4.0.3",
    "vue": "^2.6.8",
    "vue-analytics": "^5.16.4",
    "vue-apollo": "^3.0.0-beta.30",
    "vue-async-computed": "^3.6.1",
    "vue-line-clamp": "^1.3.2",
    "vue-resize-directive": "^1.2.0",
    "vue-resource": "^1.5.0",
    "vue-router": "^3.0.2",
    "vuetify": "^1.5.6",
    "vuex": "^3.1.0",
    "xml2js": "^0.4.19",
    "xxhashjs": "^0.2.2"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "esm": "^3.2.25"
  }
}
